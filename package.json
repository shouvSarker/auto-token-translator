    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "type-coverage": "^2.19.0",
    "typescript": "^4.4.4"
  },
  "typeCoverage": {
    "atLeast": 100,
    "ignoreCatch": false,
    "strict": true,
    "detail": true
