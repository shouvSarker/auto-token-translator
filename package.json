{
  "name": "auto-token-translation",
  "version": "1.0.0",
  "description": "Automatically translate tokens powered by google translate",
  "main": "index.js",
  "private": "true",
  "author": {
    "name": "Shouvojit Sarker",
    "email": "shouvojit.sarker.shouvojit@gmail.com"
  },
  "license": "MIT",
  "scripts": {
